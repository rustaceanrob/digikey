{
  "expo": {
    "name": "Digikey",
    "slug": "hush",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/digikey_icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/digikey_splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "config": {
        "usesNonExemptEncryption": false
      },
      "buildNumber": "5",
      "infoPlist": {
        "NSFaceIDUsageDescription": "Allow $(PRODUCT_NAME) to use Face ID."
      },
      "bundleIdentifier": "com.rnetzke.hush"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/hush_logo.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.USE_BIOMETRIC",
        "android.permission.USE_FINGERPRINT"
      ]
    },
    "web": {
      "favicon": "./assets/digikey_icon.png"
    },
    "plugins": [
      [
        "expo-local-authentication",
        {
          "faceIDPermission": "Allow $(PRODUCT_NAME) to use Face ID."
        }
      ],
      "expo-localization"
    ],
    "extra": {
      "eas": {
        "projectId": "0f860ea5-846d-415f-b832-a7059f36bea4"
      }
    }
  }
}
